---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>Analog Drives</title>
  <link href="http://analogdrives.github.io/blog/atom.xml" rel="self"/>
  <link href="http://analogdrives.github.io/blog"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>https://analogdrives.github.io/blog</id>
  <author>
    <name>Analog Drives/name>
    <!-- <email>analogdrives@gmail.com</email> -->
  </author>

  {% for post in site.posts %}
    <entry>
      <title>{{ post.title }}</title>
      <link href="https://analogdrives.github.io{{ post.url }}"/>
      <updated>{{ post.date | date_to_xmlschema }}</updated>
      <id>https://analogdrives.github.io{{ post.id }}</id>
      <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
  {% endfor %}

</feed>
